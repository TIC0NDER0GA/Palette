<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Palette.Pref">
    <ContentPage.Content>
        <Grid>
            <Label Text="Pref"
                Grid.Column="1"
                Grid.Row="0"/>
            
            <Button Text="Back" Clicked="NavigateButton_Back"
                    Grid.Column="1"
                Grid.Row="1"></Button>
            <Button Text="Continue" Clicked="NavigateButton_Continue"
                    Grid.Column="1"
                Grid.Row="2"></Button>
        </Grid>
    </ContentPage.Content>
</ContentPage>